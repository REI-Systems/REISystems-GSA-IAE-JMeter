            <stringProp name="BeanShellSampler.query">import org.jboss.aerogear.security.otp.Totp;
//Get the secret key for the intended user
String otpKeyStr = vars.get(&quot;USER_SECRET&quot;);
log.info(otpKeyStr);

try{
	Totp totp = new Totp(otpKeyStr); //Create a new totp object with the secret key string
	String twoFactorCode = totp.now(); //Get the current one-time password that&apos;s valid at the exact moment it&apos;s needed in real-time. Updates every 30 seconds.
	vars.put(&quot;USER_OTP&quot;, twoFactorCode); //Create the variable myOTP and use it to store the temporary one-time password to enter for Login.gov access 
}
catch (Throwable ex) {
	log.error(&quot;ERROR: &quot;, ex);
	System.out.println(&quot;ERROR&quot;);
	throw ex;
}
</stringProp>
