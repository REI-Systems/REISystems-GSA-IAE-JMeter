try{
	//add to file the line	  
	log.info(FilePath);
	File file = new File(FilePath);
	fileWriter = new FileWriter(file);
	fileWriter.append(&quot;USER_EMAIL,USER_PASSWORD,USER_SECRET,USER_OTP,X_AUTH_TOKEN,USER_TYPE\n&quot;);
	fileWriter.flush();
	fileWriter.close();
}catch (Throwable ex) {
	log.error(&quot;ERROR: &quot;, ex);
	throw ex;
}



