        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Read User Login Data XLS" enabled="true">
          <stringProp name="cacheKey"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">//This code needs the &quot;poi-3.17.jar&quot; file in the JMeter lib folder to work
import org.apache.jmeter.services.FileServer;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.FileInputStream;
import org.apache.poi.hssf.usermodel.HSSFWorkbook;
import org.apache.poi.hssf.usermodel.HSSFSheet;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Cell;

String filepath = (FileServer.getFileServer().getBaseDir() + &quot;\\data\\user_login_data.xls&quot;); //Get the full filepath of the user data file

try {
	FileInputStream file = new FileInputStream(new File(filepath)); //Create a new Input Stream
	HSSFWorkbook myWorkbook = new HSSFWorkbook(file); //Create a new Workbook object
	file.close(); //Close the input stream
	HSSFSheet sheet = myWorkbook.getSheetAt(0); //Create a new sheet object for the first and only sheet in the workbook
	int rowStart = (sheet.getFirstRowNum() + 1); //Start looking for user data at row 2 to exclude the headers
	int rowEnd = sheet.getLastRowNum(); //Get the last row containing data so null rows are excluded
	for (int rowNum = rowStart; rowNum &lt;= rowEnd; rowNum++) { //Read all cells of user data one row at a time
		Row r = sheet.getRow(rowNum);	//create a new row object for each row containing user data	
		vars.put(&quot;user&quot; + rowNum + &quot;Email&quot;, r.getCell(2).getStringCellValue()); //create an Email variable for the current row number (ex. user1Email)
		vars.put(&quot;user&quot; + rowNum + &quot;PW&quot;, r.getCell(3).getStringCellValue()); //create a Password variable for the current row number (ex. user1PW)
		vars.put(&quot;user&quot; + rowNum + &quot;Secret&quot;, r.getCell(4).getStringCellValue()); //create a Secret Key variable for the current row number (ex. user1Secret)
		vars.put(&quot;user&quot; + rowNum + &quot;Status&quot;, r.getCell(5).getStringCellValue()); //create a Status variable for the current row number (ex. user1Status)
		//The value of Status is set to &quot;LOCKED&quot; by the first user so that none others interfere with the login process.
		log.info(&quot;Initial setUp Thread user&quot; + rowNum + &quot;Status=&quot; + vars.get(&quot;user&quot; + rowNum + &quot;Status&quot;)); //Log all Status variables to confirm the file is read successfully
	}

}
catch (FileNotFoundException e) 
{
  e.printStackTrace();
} 
catch (IOException e) 
{
  e.printStackTrace();
}
</stringProp>
