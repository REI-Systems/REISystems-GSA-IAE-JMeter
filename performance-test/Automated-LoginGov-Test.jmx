        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Read User Login Data XLS" enabled="true">
          <stringProp name="cacheKey"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.jmeter.services.FileServer;
import java.io.FileNotFoundException;
import java.io.IOException;
import java.io.FileInputStream;
import org.apache.poi.hssf.usermodel.HSSFWorkbook;
import org.apache.poi.hssf.usermodel.HSSFSheet;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Cell;

String filepath = (FileServer.getFileServer().getBaseDir() + &quot;\\data\\user_login_data.xls&quot;); //Get the full filepath of the user data file

try {
	FileInputStream file = new FileInputStream(new File(filepath));
	HSSFWorkbook myWorkbook = new HSSFWorkbook(file);
	file.close();
	HSSFSheet sheet = myWorkbook.getSheetAt(0);
	int rowStart = (sheet.getFirstRowNum() + 1);
	int rowEnd = sheet.getLastRowNum();
	for (int rowNum = rowStart; rowNum &lt;= rowEnd; rowNum++) {
		Row r = sheet.getRow(rowNum);		
		vars.put(&quot;user&quot; + rowNum + &quot;Status&quot;, r.getCell(5).getStringCellValue());
		log.info(&quot;Initial tearDown Thread user&quot; + rowNum + &quot;Status=&quot; + vars.get(&quot;user&quot; + rowNum + &quot;Status&quot;));
	}

}
catch (FileNotFoundException e) 
{
  e.printStackTrace();
} 
catch (IOException e) 
{
  e.printStackTrace();
}
</stringProp>
          <stringProp name="scriptLanguage">java</stringProp>
        </JSR223Sampler>
        <hashTree/>
        <IfController guiclass="IfControllerPanel" testclass="IfController" testname="If User1 is locked" enabled="true">
          <stringProp name="IfController.condition">&quot;${user1Status}&quot;==&quot;LOCKED&quot;</stringProp>
          <boolProp name="IfController.evaluateAll">false</boolProp>
          <stringProp name="TestPlan.comments">Only execute the following steps if the value of User Status read from the Excel file is &quot;LOCKED&quot;</stringProp>
        </IfController>
        <hashTree>
          <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="Set userIndex Value" enabled="true">
            <stringProp name="TestPlan.comments">***Update number to point to a different user in the data file</stringProp>
            <stringProp name="BeanShellSampler.query">vars.put(&quot;userIndex&quot;, &quot;1&quot;);</stringProp>
            <stringProp name="BeanShellSampler.filename"></stringProp>
            <stringProp name="BeanShellSampler.parameters"></stringProp>
            <boolProp name="BeanShellSampler.resetInterpreter">false</boolProp>
          </BeanShellSampler>
          <hashTree/>
          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Write to XLS to unlock user" enabled="true">
            <stringProp name="cacheKey"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.jmeter.services.FileServer;
import java.io.FileNotFoundException;
import java.io.IOException;
import org.apache.poi.hssf.usermodel.HSSFWorkbook;
import org.apache.poi.hssf.usermodel.HSSFSheet;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Cell;

	String Filepath = (FileServer.getFileServer().getBaseDir() + &quot;\\data\\user_login_data.xls&quot;); //Get the full filepath of the user data file
try {
	int userRow = Integer.parseInt(vars.get(&quot;userIndex&quot;)); //Take the userNum variable and turn it into an Integer value
	FileInputStream file = new FileInputStream(new File(Filepath)); //create a new Input Stream
	HSSFWorkbook myWorkbook = new HSSFWorkbook(file); //create a new file object
	HSSFSheet sheet = myWorkbook.getSheetAt(0); //create a new sheet object for the first and only sheet in the workbook
	Row r = sheet.getRow(userRow); //userNum is set in the &quot;Define userNum Variable&quot; Config element
	Cell myStatus = r.createCell(5);
	myStatus.setCellValue(&quot;Unlocked&quot;);  //Set the value of the Status cell on the relevant row to &quot;Unlocked&quot; so the test can be repeated without updating the data file

	vars.put(&quot;user&quot; + userRow + &quot;Status&quot;, r.getCell(5).getStringCellValue());
	log.info(&quot;Final tearDown Thread user&quot; + userRow + &quot;Status=&quot; + vars.get(&quot;user&quot; + userRow + &quot;Status&quot;));
	FileOutputStream outFile = new FileOutputStream(new File(Filepath));
	myWorkbook.write(outFile);
	outFile.close();
	
}
catch (FileNotFoundException e) 
{
  e.printStackTrace();
} 
catch (IOException e) 
