                <stringProp name="script">//This code needs the &quot;poi-3.17.jar&quot; and &quot;commons-collections4-4.1.jar&quot; files in the JMeter lib folder to work
import org.apache.jmeter.services.FileServer;
import java.io.FileNotFoundException;
import java.io.IOException;
import org.apache.poi.hssf.usermodel.HSSFWorkbook;
import org.apache.poi.hssf.usermodel.HSSFSheet;
import org.apache.poi.ss.usermodel.Row;
import org.apache.poi.ss.usermodel.Cell;

String Filepath = (FileServer.getFileServer().getBaseDir() + &quot;\\data\\user_login_data.xls&quot;); //Get the full filepath of the user data file

try {
	int userRow = Integer.parseInt(vars.get(&quot;userIndex&quot;)); //Take the userNum variable and turn it into an Integer value
	FileInputStream file = new FileInputStream(new File(Filepath)); //create a new Input Stream
	HSSFWorkbook myWorkbook = new HSSFWorkbook(file); //create a new file object
	HSSFSheet sheet = myWorkbook.getSheetAt(0); //create a new sheet object for the first and only sheet in the workbook
	Row r = sheet.getRow(userRow); //userNum is set in the &quot;Define userNum Variable&quot; Config element
	Cell myStatus = r.createCell(5); //Create a cell object at the position of the User Status column
	myStatus.setCellValue(&quot;LOCKED&quot;); //Set the value of the Status cell on the relevant row to &quot;LOCKED&quot; so no other virtual users attempt to login as the same user

	FileOutputStream outFile = new FileOutputStream(new File(Filepath)); //create a new output stream
	myWorkbook.write(outFile); //write to the file so the updated cell value is saved externally
	outFile.close(); //close the file output stream
}
catch (FileNotFoundException e) 
{
  e.printStackTrace();
} 
catch (IOException e) 
{
  e.printStackTrace();
